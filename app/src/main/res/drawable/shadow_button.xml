<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="oval">
            <solid android:color="#1E000000" /> <padding
            android:bottom="2dp"
            android:end="2dp"
            android:start="2dp"
            android:top="2dp" />
            <size
                android:width="32dp"
                android:height="32dp" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:end="2dp"
        android:start="2dp"
        android:top="2dp">
        <shape android:shape="oval">
            <solid android:color="@android:color/white" /> <size
            android:width="32dp"
            android:height="32dp" />
        </shape>
    </item>
</layer-list>